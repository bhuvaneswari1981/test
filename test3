INSTALLED_CLI_VER=$(cdk --version | grep -oP '\d+\.\d+\.\d+')

# Use importlib.metadata for Python 3.8+ (or fallback to pkg_resources for older versions)
INSTALLED_LIB_VER=$(python -c "from importlib.metadata import version; print(version('aws-cdk-lib'))")
INSTALLED_CONSTRUCTS_VER=$(python -c "from importlib.metadata import version; print(version('constructs'))")
